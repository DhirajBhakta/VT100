{
    "name": "robinhood-cli",
    "version": "1.0.0",
    "description": "terminal sharing from wherever, whenever",
    "main": "./.bin/index.js",
    "type":"module",
    "bin": {
        "rh": "./.bin/index.js"
    },
    "scripts": {
        "clean": "rm -rf .bin",
        "build": "npx rollup -c",
        "sanitize":"sed -i~ -e '2,$b' -e '/^$/d' .bin/index.js",
        "distribute": "npm run build && npm run sanitize && npm install -g .",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gandha9394/robinhood.git"
    },
    "keywords": [
        "ssh",
        "pty",
        "webterminal",
        "webrtc",
        "sockets"
    ],
    "author": "Dhiraj Bhakta",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/gandha9394/robinhood/issues"
    },
    "homepage": "https://github.com/gandha9394/robinhood#readme",
    "dependencies": {
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-typescript": "^8.3.0",
        "boxen": "^6.2.1",
        "clui": "^0.3.6",
        "colorette": "^2.0.16",
        "commander": "^9.0.0",
        "conf": "^10.1.1",
        "dotenv": "^16.0.0",
        "express": "^4.17.2",
        "figlet": "^1.5.2",
        "inquirer": "^8.2.0",
        "isomorphic-fetch": "^3.0.0",
        "minimist": "^1.2.5",
        "node-pty": "^0.10.1",
        "ora": "^6.0.1",
        "pm2": "^5.1.2",
        "ramda": "^0.28.0",
        "webrtc.io": "^0.0.4",
        "winston": "^3.5.1",
        "wrtc": "^0.4.7",
        "ws": "^8.5.0"
    },
    "devDependencies": {
        "@types/clui": "^0.3.1",
        "@types/express": "^4.17.13",
        "@types/figlet": "^1.5.4",
        "@types/inquirer": "^8.2.0",
        "@types/isomorphic-fetch": "^0.0.35",
        "@types/minimist": "^1.2.2",
        "@types/node": "^17.0.16",
        "@types/ramda": "^0.27.64",
        "@types/ws": "^8.2.2",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "concurrently": "^7.0.0",
        "eslint": "^8.9.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.3",
        "nodemon": "^2.0.15",
        "pkg": "^5.5.2",
        "rollup-plugin-includepaths": "^0.2.4",
        "typescript": "^4.5.5"
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix"
    }
}
