{
  "name": "robinhood",
  "version": "1.0.0",
  "description": "terminal sharing from wherever, whenever",
  "main": "dist/signaling_server/index.js",
  "type": "module",
  "bin": {
    "rh": "./dist/rh/index.js"
  },
  "scripts": {
    "test": "./test.sh",
    "start": "npm run start:signaling",
    "start:signaling": "concurrently \"tsc -w\" \"nodemon dist/signaling_server -debug\"",
    "start:donee": "concurrently \"tsc -w\" \"nodemon dist/donee\"",
    "start:donor": "concurrently \"tsc -w\" \"nodemon dist/donor\"",
    "start:rh": "concurrently \"tsc -w\" \"nodemon dist/rh\"",
    "dev:signaling": "npx tsc && node dist/signaling_server -debug",
    "dev:donee": "npx tsc && node dist/donee",
    "dev:donor": "npx tsc && node dist/donor",
    "dev:rh": "npx tsc && node dist/rh",
    "package:rh": "pkg . --targets node14-macos-x64 --output rh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gandha9394/robinhood.git"
  },
  "keywords": [
    "ssh",
    "pty",
    "webterminal",
    "webrtc",
    "sockets"
  ],
  "author": "Dhiraj Bhakta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gandha9394/robinhood/issues"
  },
  "homepage": "https://github.com/gandha9394/robinhood#readme",
  "dependencies": {
    "autobind": "^1.0.3",
    "clui": "^0.3.6",
    "colorette": "^2.0.16",
    "commander": "^9.0.0",
    "conf": "^10.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "figlet": "^1.5.2",
    "inquirer": "^8.2.0",
    "node-pty": "^0.10.1",
    "pm2": "^5.1.2",
    "ramda": "^0.28.0",
    "webrtc.io": "^0.0.4",
    "winston": "^3.5.1",
    "wrtc": "^0.4.7"
  },
  "devDependencies": {
    "@types/clui": "^0.3.1",
    "@types/express": "^4.17.13",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^8.2.0",
    "@types/node": "^17.0.16",
    "@types/ramda": "^0.27.64",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.2.2",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "pkg": "^5.5.2",
    "typescript": "^4.5.5"
  }
}
